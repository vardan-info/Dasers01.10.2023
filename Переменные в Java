public class Comma {
    public static void main(String[] args) {
        int k  = 50;
        int c = 40;
        int z = 30;
        int max = k;
        
        if(k < 40){
            max = c;
        }
        
        if(z < k){
            max = z;
        }
        
        System.out.println(max);
        
    }
}
