
 public class Main {
     public static void main(String[] args) {
   int number = 456;
   
   int a = number%10;//6
   int b = number/ 100;//4
   int c= number / 10%10;//5    x
   int max  = a;

   if(b > max ){
       max = b;
   }

   if (c > max){
      max = c;
   }
     System.out.print(max);
 }
 }
